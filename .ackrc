## set less-css type
--type-set=less=.less

## set json type
--type-set=json=.json

## ignore .css files (these are all generated by less-css)
#--nocss

## ignore json files
#--nojson

## ignore emacs TAGS files
--ignore-file=is:TAGS

## ignore wordpress source dirs
## --ignore-dir=wordpress/wp-includes
## --ignore-dir=wordpress/wp-admin
## --ignore-dir=wordpress/wp-content/plugins

## ignore dirs (relative)
--ignore-dir=project/target
--ignore-dir=target
--ignore-dir=build
--ignore-dir=.target
--ignore-dir=log
--ignore-dir=logs
--ignore-dir=build/reports
--ignore-dir=build/classes
--ignore-dir=out
--ignore-dir=uploads
--ignore-dir=node_modules
